                  </goals>
                </execution>
                <execution>
                  <id>default-process-test-resources</id>
                  <phase>process-test-resources</phase>
                  <goals>
                    <goal>checkin</goal>
                  </goals>
                </execution>
              </executions>
            </plugin>
          </plugins>
        </pluginManagement>
