<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.jomc.logging</groupId>
  <artifactId>jomc-logging</artifactId>
  <packaging>pom</packaging>
  <name>JOMC Logging</name>
  <version>1.2</version>
  <description>JOMC Logging is an application programming interface to logging systems with corresponding reference implementations. This is the parent module of the JOMC Logging artifacts.</description>
  <url>${jomc.artifacts.url}</url>
  <inceptionYear>2005</inceptionYear>
  <organization>
    <name>The JOMC Project</name>
    <url>http://www.jomc.org</url>
  </organization>
  <licenses>
    <license>
      <name>The JOMC Project Copyright</name>
      <url>http://www.jomc.org/LICENSE.txt</url>
      <distribution>repo</distribution>
      <comments>Simplified BSD License.</comments>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>schulte</id>
      <name>Christian Schulte</name>
      <timezone>Europe/Berlin</timezone>
      <email>cs@schulte.it</email>
    </developer>
  </developers>
  <issueManagement>
    <system>Trac</system>
    <url>http://www.jomc.org/trac</url>
  </issueManagement>
  <scm>
    <connection>scm:svn:http://svn.jomc.org/svnroot/jomc/jomc-logging/tags/jomc-logging-1.2</connection>
