              </templateParameter>
              <templateParameter>
                <key>with-vendor-copyright</key>
                <value>false</value>
                <type>java.lang.Boolean</type>
              </templateParameter>
            </templateParameters>
          </configuration>
        </plugin>
      </plugins>
