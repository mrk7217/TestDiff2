                    <id>default-taglist</id>
                    <reports>
                      <report>taglist</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <tagListOptions>
                    <tagClasses>
                      <tagClass>
                        <displayName>JDK Tags</displayName>
                        <tags>
                          <tag>
                            <matchString>JDK:</matchString>
                            <matchType>exact</matchType>
                          </tag>
                        </tags>
                      </tagClass>
                    </tagClasses>
                  </tagListOptions>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-pmd-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-pmd</id>
                    <reports>
                      <report>pmd</report>
                    </reports>
                  </reportSet>
                  <reportSet>
                    <id>default-cpd</id>
                    <reports>
                      <report>cpd</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <format>none</format>
                  <targetJdk>${maven.compiler.target}</targetJdk>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-report-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-report</id>
                    <reports>
                      <report>report</report>
                    </reports>
                  </reportSet>
                </reportSets>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-checkstyle-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-checkstyle</id>
                    <reports>
                      <report>checkstyle</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <configLocation>org/jomc/build/resources/checkstyle.xml</configLocation>
                  <enableRulesSummary>false</enableRulesSummary>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-changes-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-trac-report</id>
                    <reports>
                      <report>trac-report</report>
                    </reports>
                    <configuration>
                      <query>order=id&amp;version=${project.version}&amp;component=JOMC Logging|${project.name}</query>
                      <tracUser>${jomc.trac.username}</tracUser>
                      <tracPassword>${jomc.trac.password}</tracPassword>
                    </configuration>
                  </reportSet>
                </reportSets>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>findbugs-maven-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-findbugs</id>
                    <reports>
                      <report>findbugs</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <jvmArgs>${mojo.findbugs.jvmArgs}</jvmArgs>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>emma-maven-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-emma</id>
                    <reports>
                      <report>emma</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <!-- xml>false</xml See http://jira.codehaus.org/browse/MEMMA-19 -->
                  <dataFiles>
                    <dataFile>${project.build.directory}/coverage.ec</dataFile>
                  </dataFiles>
                  <sourcePaths>
                    <sourcePath>${project.build.sourceDirectory}</sourcePath>
                    <sourcePath>${xjcOutputDirectory}</sourcePath>
                    <sourcePath>${javaccOutputDirectory}</sourcePath>
                  </sourcePaths>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>clirr-maven-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-clirr</id>
                    <reports>
                      <report>clirr</report>
                    </reports>
                  </reportSet>
                </reportSets>
                <configuration>
                  <minSeverity>info</minSeverity>
                </configuration>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>javacc-maven-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-jjdoc</id>
                    <reports>
                      <report>jjdoc</report>
                    </reports>
                  </reportSet>
                </reportSets>
              </reportPlugin>
              <reportPlugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-plugin-plugin</artifactId>
                <reportSets>
                  <reportSet>
                    <id>default-report</id>
                    <reports>
                      <report>report</report>
                    </reports>
                  </reportSet>
                </reportSets>
              </reportPlugin>
            </reportPlugins>
            <archive>
              <index>true</index>
              <addMavenDescriptor>false</addMavenDescriptor>
              <compress>true</compress>
              <manifestEntries>
                <Build-Date>${build.date}</Build-Date>
              </manifestEntries>
            </archive>
            <archiveExcludes>
              <archiveExclude>**/coverage.xml</archiveExclude>
            </archiveExcludes>
            <validate>true</validate>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-resources-plugin</artifactId>
          <version>${maven-resources-plugin.artifact.version}</version>
          <configuration>
            <delimiters>
              <delimiter>${*}</delimiter>
            </delimiters>
            <useDefaultDelimiters>false</useDefaultDelimiters>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-release-plugin</artifactId>
          <version>${maven-release-plugin.artifact.version}</version>
          <configuration>
            <username>${jomc.scm.username}</username>
            <password>${jomc.scm.password}</password>
            <goals>deploy</goals>
            <useReleaseProfile>false</useReleaseProfile>
            <releaseProfiles>jomc-release</releaseProfiles>
            <remoteTagging>false</remoteTagging>
            <suppressCommitBeforeTag>true</suppressCommitBeforeTag>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-jar-plugin</artifactId>
          <version>${maven-jar-plugin.artifact.version}</version>
          <configuration>
            <skipIfEmpty>true</skipIfEmpty>
            <archive>
              <index>true</index>
              <addMavenDescriptor>false</addMavenDescriptor>
              <compress>true</compress>
              <manifestEntries>
                <Build-Date>${build.date}</Build-Date>
              </manifestEntries>
            </archive>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-gpg-plugin</artifactId>
          <version>${maven-gpg-plugin.artifact.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-jarsigner-plugin</artifactId>
          <version>${maven-jarsigner-plugin.artifact.version}</version>
          <configuration>
            <excludeClassifiers>
              <excludeClassifier>bin</excludeClassifier>
            </excludeClassifiers>
            <arguments>
              <argument>-J-client</argument>
              <argument>-J-Xms32m</argument>
              <argument>-J-Xmx32m</argument>
              <argument>-J-Xss2m</argument>
              <argument>-J-Dfile.encoding=${file.encoding}</argument>
              <argument>-J-Djsse.enableSNIExtension=false</argument>
              <argument>-digestalg</argument>
              <argument>SHA-256</argument>
              <argument>-tsa</argument>
              <argument>${jarsigner.tsa}</argument>
            </arguments>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-remote-resources-plugin</artifactId>
          <version>${maven-remote-resources-plugin.artifact.version}</version>
          <executions>
            <execution>
              <id>default-process</id>
              <goals>
                <goal>process</goal>
              </goals>
              <configuration>
                <resourceBundles>
                  <resourceBundle>org.jomc.build:jomc-build-resources:${jomc.build.version}</resourceBundle>
                </resourceBundles>
                <properties>
                  <buildDate>${build.date}</buildDate>
                  <with-developer-copyright>true</with-developer-copyright>
                </properties>
                <outputDirectory>${remoteResourcesOutputDirectory}</outputDirectory>
              </configuration>
            </execution>
          </executions>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-surefire-plugin</artifactId>
          <version>${surefire.version}</version>
          <configuration>
            <argLine>${maven.surefire.argLine}</argLine>
            <forkMode>once</forkMode>
            <redirectTestOutputToFile>true</redirectTestOutputToFile>
            <runOrder>random</runOrder>
            <workingDirectory>${project.build.directory}</workingDirectory>
            <includes>
              <include>**/*Test.java</include>
              <include>**/*TestSuite.java</include>
            </includes>
            <systemPropertyVariables>
              <org.jomc.modlet.ModelContext.defaultLogLevel>ALL</org.jomc.modlet.ModelContext.defaultLogLevel>
              <org.jomc.tools.JomcTool.defaultLogLevel>ALL</org.jomc.tools.JomcTool.defaultLogLevel>
              <org.jomc.ri.DefaultObjectManager.defaultLogLevel>ALL</org.jomc.ri.DefaultObjectManager.defaultLogLevel>
              <org.jomc.cli.Jomc.defaultLogLevel>ALL</org.jomc.cli.Jomc.defaultLogLevel>
              <org.jomc.cli.commands.AbstractCommand.defaultLogLevel>ALL</org.jomc.cli.commands.AbstractCommand.defaultLogLevel>
              <java.util.logging.config.file>${project.build.testOutputDirectory}/logging.properties</java.util.logging.config.file>
              <!-- Test properties. -->
              <jomc.test.resourceEncoding>${project.build.sourceEncoding}</jomc.test.resourceEncoding>
              <jomc.test.outputDirectory>${project.build.directory}</jomc.test.outputDirectory>
            </systemPropertyVariables>
          </configuration>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-surefire-report-plugin</artifactId>
          <version>${surefire.version}</version>
        </plugin>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-javadoc-plugin</artifactId>
          <version>${maven-javadoc-plugin.artifact.version}</version>
