                  <reportSet>
                    <id>default-jxr</id>
                    <reports>
                      <report>jxr</report>
                    </reports>
                  </reportSet>
                  <reportSet>
                    <id>default-test-jxr</id>
                    <reports>
                      <report>test-jxr</report>
