                <artifactId>plexus-component-javadoc</artifactId>
                <version>${plexuscontainers.version}</version>
              </tagletArtifact>
            </tagletArtifacts>
            <additionalJOption>${maven.javadoc.additionalJOption}</additionalJOption>
