                  <groups>
                    <group>
                      <title>Application Programming Interface</title>
                      <packages>org.jomc.logging</packages>
                    </group>
                    <group>
                      <title>Tests</title>
                      <packages>org.jomc.logging.it*</packages>
                    </group>
                    <group>
                      <title>Reference Implementation</title>
                      <packages>org.jomc.logging.ri*</packages>
                    </group>
                    <group>
                      <title>Reference Implementation Test Suite</title>
                      <packages>org.jomc.logging.ri.commons.test*:org.jomc.logging.ri.jdk.test*:org.jomc.logging.ri.log4j.test*:org.jomc.logging.ri.slf4j.test*</packages>
                    </group>
                  </groups>
                </configuration>
              </reportPlugin>
              <reportPlugin>
